@inproceedings{huaUltrathinCoupledInductor2021,
	title = {Ultra-thin {Coupled} {Inductor} for a {GaN}-{Based} {CRM} {Buck} {Converter}},
	url = {https://ieeexplore.ieee.org/document/9656036/citations#citations},
	doi = {10.1109/WiPDAAsia51810.2021.9656036},
	abstract = {A interleaved Buck converter with wide input voltage range and critical conduction mode based on coupled inductor is studied. Due to the limitation of high-frequency filter inductor on the size of power module, the flux distribution characteristics of coupled inductor and its influence on the volume and loss of magnetic components are deeply studied. With ultra-thin and low loss as objective, a four-pole planar coupled inductor structure based on PCB winding is proposed. Meanwhile, the design method of its structure parameters is given as well as a design example. Finally, an experimental prototype with height of only 7mm, power density of 790W/in3 and maximum efficiency of 99\% is developed. The experimental results show the effectiveness of the proposed scheme.},
	urldate = {2024-07-01},
	booktitle = {2021 {IEEE} {Workshop} on {Wide} {Bandgap} {Power} {Devices} and {Applications} in {Asia} ({WiPDA} {Asia})},
	author = {Hua, Ming and Chen, Junyu and Xu, Guolin and Wu, Hongfei},
	month = aug,
	year = {2021},
	keywords = {Magnetic flux, Windings, Buck converters, coupled inductor, Density measurement, Power system measurements, magnetic integration, Asia, Magnetic separation, planar inductor, wide-input voltage},
	pages = {138--142},
	file = {IEEE Xplore Abstract Record:C\:\\Users\\keil\\Zotero\\storage\\E4JGZECG\\citations.html:text/html},
